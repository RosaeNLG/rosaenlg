{
  "name": "rosaenlg-lambda",
  "version": "2.0.1",
  "description": "AWS Lambda functions for RosaeNLG",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist && rm -rf lib && rm -rf .serverless",
    "tsc": "tsc",
    "test": "gulp createConfForTest && nyc --reporter=lcov --reporter=text mocha --recursive",
    "build:code": "rm -rf lib && gulp copylibs && gulp copyschemas && tsc",
    "build:swagger": "gulp swagger && cd dist/ && redoc-cli bundle openApiDocumentation_merged.json",
    "build": "npm run build:code && npm run build:swagger && serverless package",
    "deploy:dev": "tsc && gulp createConfForDev && serverless deploy --stage dev",
    "deploy:prod": "gulp createConfForProd && serverless deploy --stage prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RosaeNLG/rosaenlg.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/",
      "lib"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "devDependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.858.0",
    "gulp-awspublish": "^4.1.2",
    "gulp-rename": "^2.0.0",
    "json-refs": "^3.0.15",
    "redoc-cli": "^0.10.2",
    "rosaenlg": "3.0.1",
    "s3rver": "^3.6.1",
    "serverless": "^2.28.7",
    "serverless-domain-manager": "^5.1.0",
    "serverless-jetpack": "^0.10.8",
    "serverless-plugin-common-excludes": "^3.0.1"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.3",
    "rosaenlg-packager": "2.0.1",
    "rosaenlg-server-toolkit": "2.0.1"
  },
  "keywords": [
    "RosaeNLG",
    "aws",
    "lambda"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "Apache-2.0",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/ludan"
  }
}
