{
  "name": "rosaenlg-pluralize-fr",
  "version": "3.2.1",
  "description": "Plural form for nouns in French",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "mkdirp dist && cp src/index.js dist",
    "test": "nyc --reporter=lcov --reporter=text mocha"
  },
  "devDependencies": {
    "mkdirp": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RosaeNLG/rosaenlg.git"
  },
  "bugs": {
    "url": "https://github.com/RosaeNLG/rosaenlg/issues"
  },
  "keywords": [
    "plural",
    "French",
    "pluralize"
  ],
  "nyc": {
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "author": "Ludan Stoeckl√©, swestrich",
  "license": "MIT"
}
