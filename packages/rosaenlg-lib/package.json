{
  "name": "rosaenlg-lib",
  "description": "A Natural Language Generation library - library (code) version, without templates",
  "version": "3.4.0",
  "keywords": [
    "NLG",
    "Natural Language Generation"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "maintainers": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RosaeNLG/rosaenlg.git"
  },
  "bugs": {
    "url": "https://github.com/RosaeNLG/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "dist/index.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "grammars": "gulp --gulpfile gulpfile.js/grammars.js all",
    "build:before": "gulp --gulpfile gulpfile.js/init.js init && cp src/ExoticInterface.d.ts dist/",
    "build:tsc": "node ../_helpers/fake_resources.js before_tsc && tsc && node ../_helpers/fake_resources.js after_tsc && gulp --gulpfile gulpfile.js/copyAndChangeVersion.js forNlgLib",
    "build:main": "npm run grammars && npm run build:tsc",
    "build": "npm run build:before && npm run build:main",
    "test:before": "gulp --gulpfile ../rosaenlg/gulpfile.js/copyAndChangeVersion.js forIndexJs",
    "test": "npm run test:before && nyc --reporter=lcov --reporter=text mocha --recursive"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "support",
      "dist/*-grammar.js"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "@types/numeral": "^0.0.28",
    "date-fns": "^2.16.1",
    "english-determiners": "4.4.0",
    "english-ordinals": "2.4.0",
    "english-plurals": "2.4.0",
    "english-plurals-list": "2.4.0",
    "english-verbs-gerunds": "2.4.0",
    "english-verbs-helper": "2.4.0",
    "english-verbs-irregular": "2.4.0",
    "french-adjectives-wrapper": "2.4.0",
    "french-determiners": "4.4.0",
    "french-ordinals": "4.4.0",
    "french-verbs": "4.4.0",
    "french-verbs-lefff": "2.4.0",
    "french-words": "2.4.0",
    "french-words-gender-lefff": "2.4.0",
    "german-adjectives": "4.4.0",
    "german-adjectives-dict": "2.4.0",
    "german-determiners": "4.4.0",
    "german-dict-helper": "4.4.0",
    "german-ordinals": "4.4.0",
    "german-verbs": "4.4.0",
    "german-verbs-dict": "2.4.0",
    "german-words": "4.4.0",
    "german-words-dict": "2.4.0",
    "italian-adjectives": "4.4.0",
    "italian-adjectives-dict": "2.4.0",
    "italian-determiners": "4.4.0",
    "italian-ordinals-cardinals": "4.4.0",
    "italian-verbs": "4.4.0",
    "italian-verbs-dict": "2.4.0",
    "italian-words": "4.4.0",
    "italian-words-dict": "2.4.0",
    "lefff-helper": "4.4.0",
    "morph-it-helper": "4.4.0",
    "numeral": "^2.0.6",
    "ordinal-spanish": "^0.1.3",
    "random-js": "^2.1.0",
    "rosaenlg-commons": "2.4.0",
    "rosaenlg-filter": "4.4.0",
    "rosaenlg-n2words": "2.4.0",
    "spanish-adjectives-wrapper": "2.4.0",
    "spanish-determiners": "2.4.0",
    "spanish-verbs-wrapper": "2.4.0",
    "spanish-words": "2.4.0",
    "synonym-optimizer": "4.4.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "gulp-pegjs": "^0.2.0",
    "gulp-replace": "^1.0.0"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/ludan"
  }
}
