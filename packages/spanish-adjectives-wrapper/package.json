{
  "name": "spanish-adjectives-wrapper",
  "version": "1.1.0",
  "description": "Spanish adjectives wrapper for RosaeNLG",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "nyc --reporter=lcov --reporter=text mocha",
    "build": "tsc"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "spanish-adjectives": "1.1.0"
  },
  "keywords": [
    "adjectives",
    "Spanish"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "Apache-2.0"
}
