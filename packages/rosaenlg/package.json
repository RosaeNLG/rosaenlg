{
  "name": "rosaenlg",
  "description": "A complete Natural Language Generation library based on Pug",
  "version": "1.10.1",
  "keywords": [
    "NLG",
    "Natural Language Generation"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "maintainers": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "bugs": {
    "url": "https://gitlab.com/rosaenlg-projects/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "main": "dist/index.js",
  "scripts": {
    "grammars": "gulp --gulpfile gulpfile.js/grammars.js all",
    "build:before": "gulp --gulpfile gulpfile.js/init.js init && gulp --gulpfile gulpfile.js/copylib.js all && cp src/ExoticInterface.d.ts dist/",
    "build:mainpug": "echo {} > ../rosaenlg-pug-code-gen/dist/compiledMain_client.json && echo {} > ../rosaenlg-pug-code-gen/dist/compiledMain_server.json && gulp mainpug",
    "build:tsc": "sh ../rosaenlg-pug-code-gen/fake_resources_before.sh && tsc && sh ../rosaenlg-pug-code-gen/fake_resources_after.sh",
    "build:main": "npm run grammars && npm run build:tsc && npm run build:mainpug",
    "build:after": "npm run rollup",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "doc": "gulp doc",
    "doc:publish": "gulp doc_s3",
    "test:tiny": "node --max-old-space-size=2048 ../../node_modules/mocha/bin/mocha test/test-rosaenlg/tiny.js",
    "test": "npm run test:tiny && nyc mocha --exclude test/test-rosaenlg/tiny.js",
    "rollup": "node ./fake_resources_rollup.js before && rollup --config --de_DE && rollup --config --fr_FR && rollup --config --en_US && rollup --config --it_IT && rollup --config --OTHER && node ./fake_resources_rollup.js after"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "dist/index.js",
      "examples",
      "doc",
      "lib",
      "register.js",
      "support",
      "dist/browser",
      "dist/*-grammar.js"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "@types/numeral": "^0.0.26",
    "debug": "^4.1.1",
    "english-determiners": "2.10.1",
    "english-plurals": "0.2.1",
    "english-plurals-list": "0.2.1",
    "english-verbs-gerunds": "0.2.1",
    "english-verbs-helper": "0.2.1",
    "english-verbs-irregular": "0.2.1",
    "french-adjectives": "2.10.1",
    "french-determiners": "2.10.1",
    "french-ordinals": "2.10.1",
    "french-verbs": "2.10.1",
    "french-verbs-lefff": "0.3.1",
    "french-words-gender": "2.10.1",
    "french-words-gender-lefff": "0.3.1",
    "german-adjectives": "2.10.1",
    "german-adjectives-dict": "0.3.1",
    "german-determiners": "2.10.1",
    "german-dict-helper": "2.10.1",
    "german-ordinals": "2.10.1",
    "german-verbs": "2.10.1",
    "german-verbs-dict": "0.3.1",
    "german-words": "2.10.1",
    "german-words-dict": "0.3.1",
    "italian-adjectives": "2.10.1",
    "italian-adjectives-dict": "0.3.1",
    "italian-determiners": "2.10.1",
    "italian-ordinals-cardinals": "2.10.1",
    "italian-verbs": "2.10.1",
    "italian-verbs-dict": "0.3.1",
    "italian-words": "2.10.1",
    "italian-words-dict": "0.3.1",
    "lefff-helper": "2.10.1",
    "moment": "^2.24.0",
    "morph-it-helper": "2.10.1",
    "n2words": "^1.3.2",
    "numeral": "^2.0.6",
    "pluralize-fr": "^1.0.1",
    "pug-runtime": "^2.0.4",
    "pug-strip-comments": "^1.0.3",
    "random-js": "^2.1.0",
    "rosaenlg-filter": "2.10.1",
    "rosaenlg-pug-code-gen": "3.10.1",
    "rosaenlg-pug-lexer": "5.9.1",
    "rosaenlg-pug-linker": "4.9.1",
    "rosaenlg-pug-load": "3.9.1",
    "rosaenlg-pug-parser": "6.9.1",
    "rosaenlg-yseop": "2.10.1",
    "synonym-optimizer": "2.10.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/node": "^12.12.7",
    "browserify": "^16.5.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pegjs": "^0.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "recursive-readdir-sync": "^1.0.6",
    "rollup": "^1.29.1",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-unassert": "^0.3.0",
    "source-map-support": "^0.5.16",
    "stream": "^0.0.2",
    "typescript": "^3.7.2"
  },
  "browser": {
    "fs": false
  }
}
